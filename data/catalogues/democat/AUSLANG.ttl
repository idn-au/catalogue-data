    dcterms:temporal "2008-" ;
    dcterms:title "Austlang database." ;
    dcterms:type "dataset" ;
    dcterms:accessRights <https://linked.data.gov.au/def/data-access-rights/conditional> ;
    dcat:distribution [
        a dcat:Distribution ;
        dcterms:title "Online database and website" ;
